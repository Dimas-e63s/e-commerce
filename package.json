    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.17.0",
    "lodash.chunk": "^4.2.0",
    "node-sass": "^5.0.0",
    "postcss": "^7.0.35",
    "sass": "^1.32.8",
    "simple-line-icons": "^2.5.5",
